<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.juniordev.gradleapp.mappers.BrandMapper">
  <select id="selectAll" resultType="com.juniordev.gradleapp.beans.Brand">
    select
      brand_id 
      , brand_name 
    from
      brand b
    order by
      brand_id
  </select>
</mapper>